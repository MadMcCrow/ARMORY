{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
      {
        "name": "(gdb) godot launch",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/godot.bin",
        "preLaunchTask": "build", 
        "args": ["${workspaceFolder}/armory/project.godot", "--editor", "--upwards"],
        "cwd": "${workspaceFolder}/fps",
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }
        ]
      }
    ]
}